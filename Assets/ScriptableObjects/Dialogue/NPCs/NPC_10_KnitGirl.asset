  npcDisplayName: KnitGirl
  description: 
  npcOrder: {fileID: 11400000, guid: 6e0297df9d9ed0b49955a05c39087b90, type: 2}
  dialogueGroups:
  - type: 0
    lines:
    - speaker: 0
      text: "\uC548\uB155\uD558\uC138\uC694."
      displayDuration: 0
  - type: 1
    lines:
    - speaker: 0
      text: "\uC77C\uBC18 \uD56B\uB3C4\uADF8 \uD558\uB098 \uC8FC\uC138\uC694."
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB0E5 \uAC00\uC7A5 \uD3C9\uBC94\uD558\uAC8C\uC694."
      displayDuration: 0
  - type: 2
    lines:
    - speaker: 0
      text: "\uC774\uAC74.."
      displayDuration: 0
    - speaker: 0
      text: "\uD478\uB4DC\uD2B8\uB7ED \uD56B\uB3C4\uADF8 \uC218\uC900\uC758 \uB9DB\uC774
        \uC544\uB2CC\uB370\uC694!?"
      displayDuration: 0
    - speaker: 0
      text: "\uB108\uBB34 \uB9DB\uC788\uC5B4\uC694!"
      displayDuration: 0
  - type: 3
    lines:
    - speaker: 0
      text: "\uC74C.. \uC81C \uC8FC\uBB38\uC774 \uB108\uBB34 \uC560\uB9E4\uBAA8\uD638
        \uD588\uB098\uBCF4\uB124\uC694..."
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB798\uB3C4 \uAC10\uC0AC\uD574\uC694"
      displayDuration: 0
  - type: 9
    lines:
    - speaker: 0
      text: "\uC694\uC0C8 \uC774\uB7F0 \uD478\uB4DC\uD2B8\uB7ED \uBCF4\uAE30 \uD798\uB4E0\uB370
        \uC624\uB79C\uB9CC\uC774\uB124\uC694"
      displayDuration: 0
    - speaker: 1
      text: "\uC800\uB3C4 \uC2DC\uC791\uD78C\uC9C0 \uC5BC\uB9C8 \uC548\uB410\uC5B4\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uC815\uB9D0\uC694? \uADF8\uB7FC \uADF8 \uC804\uC5D4 \uBB50 \uD558\uC168\uC5B4\uC694?"
      displayDuration: 0
    - speaker: 1
      text: "\uB808\uC2A4\uD1A0\uB791 \uC170\uD504\uC600\uC5B4\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uD5D0, \uADFC\uB370 \uC65C \uADF8\uB7F0 \uBD84\uC774 \uD478\uB4DC\uD2B8\uB7ED\uC744?"
      displayDuration: 0
    - speaker: 0
      text: "\uC601\uD654\uC5D0 \uB098\uC624\uB294 \uAC83\uCC98\uB7FC \uD3C9\uB860\uAC00\uC640
        \uD55C\uD310 \uD588\uB2E4\uB358?"
      displayDuration: 0
    - speaker: 1
      text: "\uD558\uD558 \uC808\uB300 \uC544\uB2C8\uC5D0\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB798\uC694? \uAD81\uAE08\uD558\uB124\uC694 \uC774\uC57C\uAE30
        \uC880 \uD574\uC8FC\uC138\uC694!"
      displayDuration: 0
    - speaker: 1
      text: "\uADF8\uB9AC \uB300\uB2E8\uD55C \uC0AC\uC5F0\uC740 \uC544\uB2C8\uC5D0\uC694."
      displayDuration: 0
    - speaker: 1
      text: "\uC624\uD788\uB824 \uC7AC\uBBF8\uC5C6\uC744\uAC74\uB370\uC694"
      displayDuration: 0
    - speaker: 1
      text: "\uC5B4\uB9B4\uB584\uBD80\uD130 \uC694\uB9AC\uD558\uB294\uAC8C \uC990\uAC70\uC6CC\uC11C"
      displayDuration: 0
    - speaker: 1
      text: "\uC81C \uC74C\uC2DD\uC744 \uBA39\uC5B4\uC8FC\uB294 \uC0AC\uB78C\uB4E4\uC774
        \uD589\uBCF5\uD574\uD558\uB294\uAC78 \uBCF4\uACE0 \uC5EC\uAE30\uAE4C\uC9C0
        \uC654\uC8E0."
      displayDuration: 0
    - speaker: 1
      text: "\uADF8\uB7EC\uB2E4\uAC00 \uC170\uD504\uB77C\uB294 \uC790\uB9AC\uAE4C\uC9C0
        \uAC14\uACE0\uC694."
      displayDuration: 0
    - speaker: 1
      text: "\uADF8\uB7F0\uB370 \uB9C9\uC0C1 \uADF8\uB7F0 \uC790\uB9AC\uC5D0 \uC624\uB974\uAE30
        \uC704\uD574 \uB178\uB825\uD558\uBA74\uC11C \uC0AC\uB78C\uC774 \uB9CE\uC774
        \uBC14\uB00C\uC5C8\uC5B4\uC694."
      displayDuration: 0
    - speaker: 1
      text: "\uB354\uC774\uC0C1 \uD589\uBCF5\uD558\uC9C0\uB3C4 \uC54A\uACE0, \uC990\uAC81\uC9C0\uB3C4
        \uC54A\uC558\uC8E0"
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB7AC\uAD70\uC694..."
      displayDuration: 0
    - speaker: 1
      text: "\uC81C \uC694\uB9AC\uB97C \uC81C\uC77C \uC0AC\uB791\uD574\uC8FC\uB358
        \uC0AC\uB78C\uACFC\uB3C4 \uC810\uC810 \uBA40\uC5B4\uC838\uBC84\uB9AC\uAC8C
        \uB418\uACE0 \uC0C1\uCC98\uC785\uD614\uC8E0."
      displayDuration: 0
    - speaker: 0
      text: "\uC8C4\uC1A1\uD574\uC694... \uC81C\uAC00 \uB208\uCE58 \uC5C6\uC774 \uBB3C\uC5B4\uBD24\uAD70\uC694.."
      displayDuration: 0
    - speaker: 1
      text: "\uAD1C\uCC2E\uC544\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB7FC \uADF8 \uC18C\uC911\uD55C \uC0AC\uB78C\uC740 \uC9C0\uAE08\uC740
        \uAD1C\uCC2E\uC544\uC84C\uB098\uC694?"
      displayDuration: 0
    - speaker: 1
      text: "\uB124 \uADF8\uB7F4\uAEBC\uC5D0\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uC2DC\uAC04\uC774 \uBC8C\uC368 \uC774\uB807\uAC8C \uB410\uB124\uC694."
      displayDuration: 0
    - speaker: 0
      text: "\uC774\uC57C\uAE30 \uD574\uC918\uC11C \uACE0\uB9C8\uC6CC\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uC804 \uC774\uB9CC \uAC00\uBD10\uC57C\uACA0\uC5B4\uC694"
      displayDuration: 0
    - speaker: 1
      text: "\uB135 \uD558\uB8E8 \uC798 \uBCF4\uB0B4\uC138\uC694"
      displayDuration: 0
  - type: 9
    lines:
    - speaker: 0
      text: "\uC694\uC0C8 \uC774\uB7F0 \uD478\uB4DC\uD2B8\uB7ED \uBCF4\uAE30 \uD798\uB4E0\uB370
        \uC624\uB79C\uB9CC\uC774\uB124\uC694"
      displayDuration: 0
    - speaker: 1
      text: "\uC800\uB3C4 \uC2DC\uC791\uD78C\uC9C0 \uC5BC\uB9C8 \uC548\uB410\uC5B4\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uC815\uB9D0\uC694? \uADF8\uB7FC \uADF8 \uC804\uC5D4 \uBB50 \uD558\uC168\uC5B4\uC694?"
      displayDuration: 0
    - speaker: 1
      text: "\uB808\uC2A4\uD1A0\uB791 \uC170\uD504\uC600\uC5B4\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uD5D0, \uADFC\uB370 \uC65C \uADF8\uB7F0 \uBD84\uC774 \uD478\uB4DC\uD2B8\uB7ED\uC744?"
      displayDuration: 0
    - speaker: 0
      text: "\uC601\uD654\uC5D0 \uB098\uC624\uB294 \uAC83\uCC98\uB7FC \uD3C9\uB860\uAC00\uC640
        \uD55C\uD310 \uD588\uB2E4\uB358?"
      displayDuration: 0
    - speaker: 1
      text: "\uD558\uD558 \uC808\uB300 \uC544\uB2C8\uC5D0\uC694"
      displayDuration: 0
    - speaker: 0
      text: "\uADF8\uB798\uC694? \uAD81\uAE08\uD558\uB124\uC694 \uC774\uC57C\uAE30
        \uC880 \uD574\uC8FC\uC138\uC694!"
      displayDuration: 0
    - speaker: 1
      text: "\uADF8\uB9AC \uB300\uB2E8\uD55C \uC0AC\uC5F0\uC740 \uC544\uB2C8\uC5D0\uC694."
      displayDuration: 0
